  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Sakai
  pod 'AccordionMenuSwift', '~> 1.2.5'
	
  target 'SakaiTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SakaiUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
